    <entity type="SamplePlaceable" src="script/ide/IPCBinding/EntitySamplePlaceableTemplate.entity.xml" display_property="mesh_name"/>
  </folder>
  <folder name="Animated">
    <entity type="MiniGame" src=""/>
    <entity type="RegionSound" src=""/>
    <entity type="PickableObject" src=""/>
  </folder>
  <folder name="worlds">
    <entity type="HomeWorld" src=""/>
    <entity type="PublicWorld" src=""/>
  </folder>
  <folder name="Models">
