    <entity type="Sound" tooltip="Sound" targetfolder="Sound" display_property="name" src="script/PETools/Aries/Sound.entity.xml"/>
    <entity type="AudioSource" tooltip="AudioSource" targetfolder="AudioSource" display_property="name" src="script/PETools/Aries/AudioSource.entity.xml"/>
    <entity type="FootStepsScene" tooltip="FootSteps Scene" targetfolder="FootStepsScene" display_property="name" src="script/PETools/Aries/FootStepsScene.entity.xml"/>
  </folder>
  <folder name="Taurus" IsExpanded="true">
    <entity type="EnvironmentPresets" tooltip="Environment Presets" targetfolder="EnvironmentPresets" display_property="name" src="script/PETools/Taurus/EnvironmentPresets.entity.xml"/>
